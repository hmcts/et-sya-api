{
  "provider": {
    "name": "s2-auth-api"
  },
  "consumer": {
    "name": "et_sya_api_service"
  },
  "interactions": [
    {
      "description": "Provider receives a token request request from et-sya-api API",
      "request": {
        "method": "GET",
        "path": "/lease"
      },
      "response": {
        "status": 200,
        "headers": {
          "Authorization": "someToken",
          "Content-Type": "application/json; charset=UTF-8"
        },
        "body": {
          "token": "someMicroServiceToken"
        },
        "matchingRules": {
          "body": {
            "$.token": {
              "matchers": [
                {
                  "match": "type"
                }
              ],
              "combine": "AND"
            }
          },
          "header": {
            "Content-Type": {
              "matchers": [
                {
                  "match": "regex",
                  "regex": "application/json(;\\s?charset=[\\w\\-]+)?"
                }
              ],
              "combine": "AND"
            }
          }
        }
      },
      "providerStates": [
        {
          "name": "a secret exists"
        }
      ]
    }
  ],
  "metadata": {
    "pactSpecification": {
      "version": "3.0.0"
    },
    "pact-jvm": {
      "version": "4.1.41"
    }
  }
}
